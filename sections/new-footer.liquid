{{ 'new-footer.css' | asset_url | stylesheet_tag }}

{% liquid
  assign footer_links = section.settings.footer_links.links
  assign instagram = section.settings.instagram
  assign tik_tok = section.settings.tik_tok
  assign twitter = section.settings.twitter
  assign input_text = section.settings.input_text
%}

<footer class="new-footer">
  <div class="new-footer-wrapper">
    <div class="new-footer-links">
      {% for link in footer_links %}
        <a href="{{ link.url }}" class="new-footer-link">
          {{ link.title }}
        </a>
      {% endfor %}
    </div>

    <div class="new-footer-signup">
      <div
        class="email-signup-block size-style spacing-style"
      >
        <h4 class="email-signup-label">
          {{ input_text }}
        </h4>
        {%- form 'customer', class: 'email-signup__form spacing-style' %}
          <div
            class="email-signup__input-group"
            style="--border-width:"
          >
            <label
              for="EmailInput-{{ section.id }}"
              class="visually-hidden"
            >
              {{ 'blocks.email_signup.label' | t }}
            </label>
            <input
              id="EmailInput-{{ section.id }}"
              class="email-signup__input"
              type="email"
              name="contact[email]"
              autocorrect="off"
              autocapitalize="off"
              autocomplete="email"
              placeholder="Email"
              required
              {% if form.errors %}
                autofocus
                aria-invalid="true"
                aria-describedby="Email-signup__message-error-{{ section.id }}"
              {% elsif form.posted_successfully? %}
                aria-describedby="Email-signup__message-success-{{ section.id }}"
              {% endif %}
            >
            <button
              class="email-signup__button"
            >
              <svg width="14" height="10" viewBox="0 0 14 10" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path fill-rule="evenodd" clip-rule="evenodd" d="M8.61107 0.33169C8.64471 0.250013 8.69803 0.177923 8.76627 0.121837C8.83452 0.0657517 8.91557 0.0274116 9.00222 0.0102308C9.08886 -0.00695007 9.17841 -0.00243849 9.2629 0.0233638C9.34738 0.049166 9.42417 0.0954583 9.48643 0.158119L13.7721 4.44383C13.822 4.4936 13.8616 4.55271 13.8886 4.6178C13.9156 4.68288 13.9295 4.75265 13.9295 4.82312C13.9295 4.89358 13.9156 4.96336 13.8886 5.02844C13.8616 5.09352 13.822 5.15264 13.7721 5.2024L9.48643 9.48812C9.38584 9.58871 9.2494 9.64522 9.10714 9.64522C8.96488 9.64522 8.82845 9.58871 8.72786 9.48812C8.62726 9.38753 8.57075 9.25109 8.57075 9.10883C8.57075 8.96657 8.62726 8.83014 8.72786 8.72955L12.0996 5.35883H0.535714C0.393634 5.35883 0.257373 5.30239 0.156907 5.20193C0.0564411 5.10146 0 4.9652 0 4.82312C0 4.68104 0.0564411 4.54478 0.156907 4.44431C0.257373 4.34385 0.393634 4.2874 0.535714 4.2874H12.0996L8.72786 0.91669C8.65259 0.841743 8.6013 0.746113 8.5805 0.64195C8.55971 0.537786 8.57035 0.429793 8.61107 0.33169Z" fill="black"/>
              </svg>
            </button>
          </div>

          {%- if form.errors -%}
            <div
              class="email-signup__message email-signup__message--error"
              tabindex="-1"
            >
              <svg
                viewBox="0 0 20 20"
                xmlns="http://www.w3.org/2000/svg"
                focusable="false"
                aria-hidden="true"
                class="icon-error"
              >
                {%- render 'icon', icon: 'error'  -%}
              </svg>
              {{- form.errors.translated_fields.email | capitalize }}
              {{ form.errors.messages.email -}}
            </div>
          {%- endif -%}

          {%- if form.posted_successfully? -%}
            <div
              class="email-signup__message email-signup__message--success"
              tabindex="-1"
            >
              <span class="svg-wrapper icon-success">
                {{ 'icon-checkmark.svg' | inline_asset_content }}
              </span>
              <p class="email-signup__message-text">{{ 'blocks.email_signup.success' | t }}</p>
            </div>
          {%- endif -%}
        {% endform %}
      </div>
